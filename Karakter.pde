class Karakter{
  
  int baseAttackSpeed, attackSpeedGrowth;
  int ad, ap;
  int crit;
  
  
  Karakter(int baseAttackSpeed, int attackSpeedGrowth, int ad, int ap, int crit){
    this.baseAttackSpeed = baseAttackSpeed;
    this.attackSpeedGrowth = attackSpeedGrowth;
    this.ad = ad;
    this.ap = ap;
    this.crit = crit;
    
  }
  
  
}
